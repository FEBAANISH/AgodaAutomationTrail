 package interviewquestions;

public class FactorialornotNonRecurssive {

	public static void main(String[] args) {
		System.out.println(factorialcheck(5));
		System.out.println(factorialcheck(0));

	}
	
	public static int factorialcheck(int num)
	{
		int fact = 1;
		if(num==0)
		
			return 1;
		for(int i=1;i<=num;i++)
		{
			fact =fact*i;
		}
		return fact;
		
	}

}
