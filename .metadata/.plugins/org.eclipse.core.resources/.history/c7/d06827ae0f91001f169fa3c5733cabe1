package interviewquestions;

public class check {
	
	public static void main(String[] args)
	{
		int[] num = {1,2,120,13,500};
		int largest =num[0];
		int smallest = num[0];
		int len = num.length;
		
		for (int i =1;i<len;i++)
		{
			if(num[i]>largest)
			{
				largest= num[i];
			}
			if(num[i]<smallest)
			{
				smallest =num[i];
			}
		}
		System.out.println("the largest number"+ largest);
		System.out.println("the smallest number"+ smallest);
		
		
	}

	
}
